object SolutionHW2 {
  def MySum(a: List[Int]): Int =
    a.foldLeft(0)(_ + _)

  def MyRange(n: Int, m: Int): List[Int] =
    if (n > m) Nil
    else if (n == m) List(n)
    else n :: MyRange(n + 1, m)

  def main(args: Array[String]): Unit = {
    println(MySum(MyRange(1, 5)))
    println(MySum(MyRange(1, 1)))
    println(MySum(MyRange(5, 1)))
  }
}
